package components

templ BooksList(books []string) {
	<section>
		<style> 
        me {
            height: 100vh;
            padding: 20px;
        }
        </style>
		for _, book := range books {
			<article>
				<h2>{ book }</h2>
				<p>Some description of the book</p>
			</article>
		}
	</section>
}
